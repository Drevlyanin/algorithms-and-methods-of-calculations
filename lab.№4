#include <iostream>
using namespace std;

int main()
{
    float A[2][3];
    int i, j, k = 0, S = 0;

    for(i = 0; i < 2; i++) {
        for(j = 0; j < 3; j++) {
            cin >> A[i][j];
            if(A[i][j] != 0) {
                k++; 
                S = S + A[i][j];
            }
        }
    }
    
    cout << "k=" << k << " S=0" << S;
    
    return 0;
}
